{
  "$schema": "https://cdn.deno.land/denon/versions/2.4.0/raw/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run --allow-net --allow-write --allow-read --allow-plugin --unstable --config=tsconfig.json mod.ts",
      "desc": "run my app.ts file"
    }
  },
  "watcher": {
    "match": ["**/*"]
  }
}
